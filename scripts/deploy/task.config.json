{
	"containerDefinitions": [{
		"name": "ui",
		"image": "614459117250.dkr.ecr.eu-west-1.amazonaws.com/eyewitness-ui:latest",
		"memoryReservation": 128,
		"essential": true,
		"portMappings": [{
			"containerPort": 5002,
			"hostPort": 0,
			"protocol": "tcp"
		}],
		"environment": [{
			"name": "NODE_ENV",
			"value": "{{REPLACE_ME}}"
		}, {
			"name": "PROVIDER_ID",
			"value": "{{REPLACE_ME}}"
		}],
		"logConfiguration": {
			"logDriver": "awslogs",
			"options": {
				"awslogs-region": "{{REPLACE_ME}}",
				"awslogs-group": "{{REPLACE_ME}}",
				"awslogs-stream-prefix": "ui"
			}
		}
	}]
}
