{
 "name": "eyewitness-ui",
 "version": "0.0.1",
 "private": true,
 "description": "User interface to allow admins to interact with their Eyewitness bot.",
 "main": "app/backend/main.js",
 "scripts": {
  "ngrok": "ngrok http 6000 --region eu -subdomain=eyewitness",
  "start-local": "concurrently \"npm run watch\" \"trawler --debug --stdall\"",
  "start-development": "npm install && npm run start-local",
  "start-production": "trawler --stdall --plain",
  "build": "npm run lint && webpack --display-error-details",
  "watch": "webpack --display-error-details --watch",
  "lint": "eslint --ext js,vue ./app/backend ./app/frontend/source",
  "test": "echo \"Tests not implemented!\""
 },
 "author": "Atchai <enquiries@atchai.com> (https://atchai.com)",
 "dependencies": {
  "body-parser": "^1.18.2",
  "compression": "^1.7.1",
  "config-ninja": "^1.1.0",
  "cookie-parser": "^1.4.3",
  "express": "^4.16.2",
  "express-basic-auth": "^1.1.3",
  "express-handlebars": "^3.0.0",
  "eyewitness": "git+ssh://git@github.com/atchai/eyewitness.git",
  "moment": "^2.19.2",
  "socket.io": "^2.0.4",
  "trawler-std": "^0.3.7"
 },
 "devDependencies": {
  "autoprefixer": "^7.1.6",
  "babel-core": "^6.26.0",
  "babel-loader": "^7.1.2",
  "babel-minify-webpack-plugin": "^0.2.0",
  "babel-preset-env": "^1.6.1",
  "bson-objectid": "^1.2.1",
  "clean-webpack-plugin": "^0.1.17",
  "concurrently": "^3.5.0",
  "css-loader": "^0.28.7",
  "dialog-polyfill": "^0.4.9",
  "eslint": "^4.11.0",
  "eslint-config-recombix": "^0.3.2",
  "eslint-config-vue": "latest",
  "eslint-plugin-disable": "latest",
  "eslint-plugin-filenames": "latest",
  "eslint-plugin-html": "latest",
  "eslint-plugin-json": "latest",
  "eslint-plugin-node": "latest",
  "eslint-plugin-promise": "latest",
  "eslint-plugin-vue": "latest",
  "extract-text-webpack-plugin": "^3.0.2",
  "html-webpack-plugin": "^2.30.1",
  "ngrok": "^2.2.23",
  "node-sass": "^4.7.2",
  "nodemon": "^1.12.1",
  "normalize.css": "^7.0.0",
  "postcss-loader": "^2.0.8",
  "sass-loader": "^6.0.6",
  "sass-resources-loader": "^1.3.1",
  "socket.io-client": "^2.0.4",
  "style-loader": "^0.19.0",
  "vue": "^2.5.3",
  "vue-loader": "^13.5.0",
  "vue-router": "^3.0.1",
  "vue-template-compiler": "^2.5.3",
  "vuex": "^3.0.1",
  "webpack": "^3.8.1"
 },
 "trawler": {
  "crash": {
   "autoRestart": true,
   "waitSourceChange": true
  },
  "sourceChange": {
   "autoRestart": true,
   "environments": [
    "development"
   ],
   "watched": [
    "package.json",
    "package-lock.json",
    "app"
   ]
  },
  "console": {
   "stdout": true,
   "stderr": true
  }
 },
 "engines": {
  "node": ">=8"
 },
 "browserslist": [
  "last 2 versions",
  "> 0.5%"
 ]
}
