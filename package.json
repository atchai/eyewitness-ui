{
 "name": "eyewitness-ui",
 "version": "0.0.1",
 "private": true,
 "description": "User interface to allow admins to interact with their Eyewitness bot.",
 "main": "app/backend/main.js",
 "scripts": {
  "ngrok": "ngrok http 6000 --region eu -subdomain=eyewitness",
  "start-local": "concurrently \"npm run watch\" \"trawler --debug --stdall\"",
  "start-development": "npm install && npm update -S && npm run start-local",
  "start-production": "trawler --stdall --plain",
  "build": "webpack --display-error-details",
  "watch": "webpack --display-error-details --watch",
  "test": "echo \"Tests not implemented!\""
 },
 "author": "Atchai <enquiries@atchai.com> (https://atchai.com)",
 "dependencies": {
  "body-parser": "^1.18.2",
  "clean-webpack-plugin": "^0.1.17",
  "compression": "^1.7.1",
  "config-ninja": "^1.1.0",
  "cookie-parser": "^1.4.3",
  "express": "^4.16.2",
  "express-basic-auth": "^1.1.3",
  "express-handlebars": "^3.0.0",
  "eyewitness": "git+ssh://git@github.com/atchai/eyewitness.git#master",
  "html-webpack-plugin": "^2.30.1",
  "moment": "^2.19.1",
  "trawler-std": "^0.3.7",
  "vue": "^2.5.3",
  "webpack": "^3.8.1"
 },
 "devDependencies": {
  "eslint": "latest",
  "eslint-config-recombix": "^0.2.6",
  "eslint-plugin-filenames": "latest",
  "eslint-plugin-json": "latest",
  "eslint-plugin-node": "latest",
  "eslint-plugin-promise": "latest",
  "ngrok": "^2.2.23",
  "nodemon": "^1.12.1"
 },
 "trawler": {
  "crash": {
   "autoRestart": true,
   "waitSourceChange": true
  },
  "sourceChange": {
   "autoRestart": true,
   "environments": [
    "development"
   ],
   "watched": [
    "package.json",
    "package-lock.json",
    "app"
   ]
  },
  "console": {
   "stdout": true,
   "stderr": true
  }
 },
 "engines": {
  "node": ">=8"
 }
}
